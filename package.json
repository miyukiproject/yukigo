{
  "name": "yukigo-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm build --workspaces --if-present",
    "test": "npm test --workspaces --if-present",
    "build:ast": "npm run build -w @yukigo/ast",
    "docs:dev": "npm run build -w @yukigo/ast & npm run build & npm run test & npm run docs:dev --workspace=@yukigo/docs",
    "docs:gen": "npm run docs:gen --workspace=@yukigo/docs"
  },
  "dependencies": {
    "ts-morph": "^27.0.2"
  }
}
