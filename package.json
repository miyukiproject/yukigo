{
  "name": "yukigo-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "typecheck": "nx run-many -t typecheck",
    "graph": "nx graph",
    "docs:dev": "nx run-many -t build --parallel=3 & npm run docs:dev --workspace=yukigo-docs",
    "docs:gen": "npm run docs:gen --workspace=yukigo-docs"
  },
  "dependencies": {
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@nx/js": "^22.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "^1.15.11",
    "@swc/helpers": "~0.5.11",
    "mocha": "^11.7.5",
    "nx": "22.1.3",
    "vite": "^7.3.1",
    "vitepress": "^1.6.4"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "diff": "^5.2.0"
  }
}